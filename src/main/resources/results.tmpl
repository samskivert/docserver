<html>
<head>
  <title>Docserver: {{query}}</title>
  <style>
  body {
    font-family: sans-serif;
  }
  </style>
</head>
<body>
  <h1>Query: {{query}}</h1>

{{#results.isEmpty}}
  <p>No matches.</p>
{{/results.isEmpty}}

  <form method="get">
    <p>New Query: <input type="text" name="search" value="{{query}}"/>
    <input type="submit" name="Go" value="Go"/></p>
  </form>

{{^results.isEmpty}}
  <table>
{{/results.isEmpty}}
{{#results}}
  <tr><td>
{{^artifact.docJar.isEmpty}}
  <a href="docs/{{artifact.pom.fqId}}/{{entry.docPath}}">{{{qname}}}</a>
{{/artifact.docJar.isEmpty}}
{{#artifact.docJar.isEmpty}}
  {{{qname}}}
{{/artifact.docJar.isEmpty}}
{{^artifact.sourceJar.isEmpty}}
  <a href="source/{{artifact.pom.fqId}}/{{entry.sourcePath}}">[source]</a>
{{/artifact.sourceJar.isEmpty}}
  </td><td>{{artifact.pom.artifactId}} : {{artifact.pom.version}}</td></tr>
{{/results}}
{{^results.isEmpty}}
  </table>
{{/results.isEmpty}}
</body>
</html>
